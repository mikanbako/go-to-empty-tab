{
  "manifest_version": 2,
  "name": "List tabs",
  "description": "Lists properties of all tabs to log",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "List tabs"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}