{
  "manifest_version": 2,
  "name": "Go to empty tab",
  "description": "Switches to a next or a previous empty tab.",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "Go to empty tab"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}