{
  "manifest_version": 2,
  "name": "Go to empty tab",
  "description": "Switches to a next or a previous empty tab.",
  "version": "0.0.1",
  "icons": {
    "48": "icons/icon.svg",
    "64": "icons/icon.svg",
    "96": "icons/icon.svg"
},
  "browser_action": {
    "default_icon": {
      "48": "icons/icon.svg",
      "64": "icons/icon.svg",
      "96": "icons/icon.svg"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "Go to empty tab"
  },
  "permissions": [
    "tabs"
  ]
}